{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "AuthorizationRules_RootManageSharedAccessKey_name": {
            "defaultValue": "RootManageSharedAccessKey",
            "type": "String"
        },
        "AuthorizationRules_lab_name": {
            "defaultValue": "lab",
            "type": "String"
        },
        "consumergroups_crimes_chicago_cg_name": {
            "defaultValue": "crimes_chicago_cg",
            "type": "String"
        },
        "consumergroups_$Default_name": {
            "defaultValue": "$Default",
            "type": "String"
        },
        "authorizationRules_lab_name_1": {
            "defaultValue": "lab",
            "type": "String"
        },
        "consumergroups_$Default_name_1": {
            "defaultValue": "$Default",
            "type": "String"
        },
        "namespaces_gws_lab_name": {
            "defaultValue": "gws-lab",
            "type": "String"
        },
        "eventhubs_lab_name": {
            "defaultValue": "lab",
            "type": "String"
        },
        "eventhubs_crimes_aeh_name": {
            "defaultValue": "crimes_aeh",
            "type": "String"
        },
        "location": {
            "type": "string",
            "defaultValue": "[resourceGroup().location]",
            "metadata": {
                "description": "Location for all resources."
            }
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.EventHub/namespaces",
            "sku": {
                "name": "Standard",
                "tier": "Standard",
                "capacity": 3
            },
            "name": "[parameters('namespaces_gws_lab_name')]",
            "apiVersion": "2017-04-01",
            "location": "[parameters('location')]",
            "tags": {},
            "scale": null,
            "properties": {
                "isAutoInflateEnabled": false,
                "maximumThroughputUnits": 0
            },
            "dependsOn": []
        },
        {
            "type": "Microsoft.EventHub/namespaces/AuthorizationRules",
            "name": "[concat(parameters('namespaces_gws_lab_name'), '/', parameters('AuthorizationRules_RootManageSharedAccessKey_name'))]",
            "apiVersion": "2017-04-01",
            "location": "[parameters('location')]",
            "scale": null,
            "properties": {
                "rights": [
                    "Listen",
                    "Manage",
                    "Send"
                ]
            },
            "dependsOn": [
                "[resourceId('Microsoft.EventHub/namespaces', parameters('namespaces_gws_lab_name'))]"
            ]
        },
        {
            "type": "Microsoft.EventHub/namespaces/AuthorizationRules",
            "name": "[concat(parameters('namespaces_gws_lab_name'), '/', parameters('AuthorizationRules_lab_name'))]",
            "apiVersion": "2017-04-01",
            "location": "[parameters('location')]",
            "scale": null,
            "properties": {
                "rights": [
                    "Listen",
                    "Send"
                ]
            },
            "dependsOn": [
                "[resourceId('Microsoft.EventHub/namespaces', parameters('namespaces_gws_lab_name'))]"
            ]
        },
        {
            "type": "Microsoft.EventHub/namespaces/eventhubs",
            "name": "[concat(parameters('namespaces_gws_lab_name'), '/', parameters('eventhubs_crimes_aeh_name'))]",
            "apiVersion": "2017-04-01",
            "location": "[parameters('location')]",
            "scale": null,
            "properties": {
                "messageRetentionInDays": 1,
                "partitionCount": 32,
                "status": "Active"
            },
            "dependsOn": [
                "[resourceId('Microsoft.EventHub/namespaces', parameters('namespaces_gws_lab_name'))]"
            ]
        },
        {
            "type": "Microsoft.EventHub/namespaces/eventhubs",
            "name": "[concat(parameters('namespaces_gws_lab_name'), '/', parameters('eventhubs_lab_name'))]",
            "apiVersion": "2017-04-01",
            "location": "[parameters('location')]",
            "scale": null,
            "properties": {
                "messageRetentionInDays": 1,
                "partitionCount": 2,
                "status": "Active"
            },
            "dependsOn": [
                "[resourceId('Microsoft.EventHub/namespaces', parameters('namespaces_gws_lab_name'))]"
            ]
        },
        {
            "type": "Microsoft.EventHub/namespaces/eventhubs/authorizationRules",
            "name": "[concat(parameters('namespaces_gws_lab_name'), '/', parameters('eventhubs_crimes_aeh_name'), '/', parameters('authorizationRules_lab_name_1'))]",
            "apiVersion": "2017-04-01",
            "location": "[parameters('location')]",
            "scale": null,
            "properties": {
                "rights": [
                    "Manage",
                    "Listen",
                    "Send"
                ]
            },
            "dependsOn": [
                "[resourceId('Microsoft.EventHub/namespaces', parameters('namespaces_gws_lab_name'))]",
                "[resourceId('Microsoft.EventHub/namespaces/eventhubs', parameters('namespaces_gws_lab_name'), parameters('eventhubs_crimes_aeh_name'))]"
            ]
        },
        {
            "type": "Microsoft.EventHub/namespaces/eventhubs/consumergroups",
            "name": "[concat(parameters('namespaces_gws_lab_name'), '/', parameters('eventhubs_crimes_aeh_name'), '/', parameters('consumergroups_$Default_name_1'))]",
            "apiVersion": "2017-04-01",
            "location": "[parameters('location')]",
            "scale": null,
            "properties": {},
            "dependsOn": [
                "[resourceId('Microsoft.EventHub/namespaces', parameters('namespaces_gws_lab_name'))]",
                "[resourceId('Microsoft.EventHub/namespaces/eventhubs', parameters('namespaces_gws_lab_name'), parameters('eventhubs_crimes_aeh_name'))]"
            ]
        },
        {
            "type": "Microsoft.EventHub/namespaces/eventhubs/consumergroups",
            "name": "[concat(parameters('namespaces_gws_lab_name'), '/', parameters('eventhubs_crimes_aeh_name'), '/', parameters('consumergroups_crimes_chicago_cg_name'))]",
            "apiVersion": "2017-04-01",
            "location": "[parameters('location')]",
            "scale": null,
            "properties": {},
            "dependsOn": [
                "[resourceId('Microsoft.EventHub/namespaces', parameters('namespaces_gws_lab_name'))]",
                "[resourceId('Microsoft.EventHub/namespaces/eventhubs', parameters('namespaces_gws_lab_name'), parameters('eventhubs_crimes_aeh_name'))]"
            ]
        },
        {
            "type": "Microsoft.EventHub/namespaces/eventhubs/consumergroups",
            "name": "[concat(parameters('namespaces_gws_lab_name'), '/', parameters('eventhubs_lab_name'), '/', parameters('consumergroups_$Default_name'))]",
            "apiVersion": "2017-04-01",
            "location": "[parameters('location')]",
            "scale": null,
            "properties": {},
            "dependsOn": [
                "[resourceId('Microsoft.EventHub/namespaces', parameters('namespaces_gws_lab_name'))]",
                "[resourceId('Microsoft.EventHub/namespaces/eventhubs', parameters('namespaces_gws_lab_name'), parameters('eventhubs_lab_name'))]"
            ]
        }
    ],
    "outputs": {}
}